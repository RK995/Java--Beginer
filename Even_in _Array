package Array;

public class even_in_Array {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] n= new int [] {
			8,10,15,20,25,50,55,72	
		};
		for (int i=0; i<n.length;i++) {
			if(n[i]%2==0) {
				System.out.println("Yes");
			}
			else {
				System.out.println("No");
			}
			
		}

	}

}
